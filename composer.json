{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "acacha/admin-lte-template-laravel": "^6.0",
        "brian2694/laravel-toastr": "^5.5",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^3.2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "milankyncl/ares-api": "dev-master",
        "realrashid/sweet-alert": "^1.1",
        "rebing/graphql-laravel": "^3.1",
        "select2/select2": "^4.0",
        "tightenco/ziggy": "^0.8.0",
        "yajra/laravel-datatables-buttons": "^3.0",
        "yajra/laravel-datatables-oracle": "^8.7"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
